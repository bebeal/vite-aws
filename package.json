{
  "name": "vite-aws",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/bebeal/vite-aws"
  },
  "bugs": {
    "url": "https://github.com/bebeal/vite-aws/issues"
  },
  "scripts": {
    "build": "yarn clean && NODE_ENV=production yarn build:client && yarn build:server && yarn postbuild",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p tsconfig.node.json && vite build --ssr src/entry-server.tsx",
    "build:cdk": "CDK_OUTDIR=cdk.out tsx cdk/cdk.ts",
    "clean": "rimraf dist cdk.out",
    "dev": "nodemon --watch server --exec tsx server",
    "deploy": "dotenv -- cdk deploy --all --require-approval never --ci true",
    "format": "prettier --write src server.ts api",
    "lint": "yarn eslint src server.ts api --report-unused-disable-directives --max-warnings 0",
    "postbuild": "chmod +x ./postbuild.sh && ./postbuild.sh",
    "serve": "node dist/server/server.js",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.901.0",
    "@aws-sdk/client-bedrock-agent": "^3.901.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.901.0",
    "@aws-sdk/client-bedrock-runtime": "^3.901.0",
    "@bebeal/rehype-code-terminal": "^0.3.2",
    "@bebeal/rehype-color-chips": "^1.0.1",
    "@bebeal/ripple-effect": "^2.1.0",
    "@carbon/icons-react": "^11.68.0",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-start": "^1.132.48",
    "compression": "^1.8.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-tweet": "^3.2.2",
    "rehype-katex": "^7.0.1",
    "rehype-starry-night": "^2.2.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-mdx-frontmatter": "^5.2.0",
    "serve-static": "^2.2.0",
    "serverless-http": "^3.2.0",
    "source-map-support": "^0.5.21",
    "vite": "^6.3.6"
  },
  "devDependencies": {
    "@bebeal/console-prefix-plugin": "^0.1.6",
    "@eslint/js": "^9.37.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/eslint-plugin-router": "^1.132.31",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.132.47",
    "@tanstack/router-plugin": "^1.132.47",
    "@types/compression": "^1.8.1",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.8",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/serve-static": "^1.15.9",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.7.0",
    "@wooorm/starry-night": "^3.8.0",
    "aws-cdk": "^2.1029.4",
    "aws-cdk-lib": "^2.219.0",
    "constructs": "^10.4.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "9.37.0",
    "eslint-plugin-mdx": "^3.6.2",
    "next-themes": "^0.4.6",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.46.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "4.52.4",
    "@rollup/rollup-linux-arm64-gnu": "4.52.4",
    "@rollup/rollup-linux-x64-gnu": "4.52.4",
    "@rollup/rollup-win32-x64-msvc": "4.52.4"
  },
  "engines": {
    "node": ">=20",
    "yarn": ">=4",
    "npm": ">=9"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "arm64",
    "x64",
    "ia32"
  ],
  "packageManager": "yarn@4.9.1"
}
